{
  "name": "typescript-lambda-example",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/handler.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist node_modules",
    "test": "jest --passWithNoTests",
    "test:dev": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand",
    "lint": "eslint src/**/*.{ts,js} tests/**/*.{ts,js} --max-warnings=0",
    "format": "prettier --write .",
    "ci": "npm run build && npm test",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "csv-parse": "^5.4.0",
    "csv-parser": "^3.0.0",
    "mssql": "^9.2.1",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.157",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.19.24",
    "@types/pg": "^8.6.7",
    "@types/sinon": "^10.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "eslint": "^8.48.0",
    "jest": "^29.6.1",
    "jest-junit": "16.0.0",
    "sinon": "^15.2.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "9.5.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3"
  }
}
